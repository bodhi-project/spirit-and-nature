{
  "name": "@bodhi-project/web-template",
  "description": "Gatsbyjs website template with advanced features.",
  "version": "0.0.1",
  "author": {
    "name": "Bodhi Project",
    "email": "hello@bodhiproject.org",
    "url": "https://www.bodhiproject.org/"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "rm -rf public && gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "dependencies": {
    "@bodhi-project/components": "0.0.49",
    "@bodhi-project/semantic-webflow": "0.0.8",
    "@bodhi-project/seo": "0.0.3",
    "@bodhi-project/typography": "0.0.13",
    "@bodhi-project/utilities": "0.0.3",
    "antd": "^2.13.10",
    "babel-plugin-lodash": "^3.3.2",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.119",
    "gatsby-link": "^1.6.28",
    "gatsby-plugin-antd": "^1.0.6",
    "gatsby-plugin-catch-links": "^1.0.9",
    "gatsby-plugin-feed": "^1.3.13",
    "gatsby-plugin-google-analytics": "^1.0.12",
    "gatsby-plugin-jss": "^1.5.6",
    "gatsby-plugin-less": "^1.0.6",
    "gatsby-plugin-manifest": "^1.0.8",
    "gatsby-plugin-nprogress": "^1.0.7",
    "gatsby-plugin-offline": "^1.0.10",
    "gatsby-plugin-react-helmet": "^2.0.1",
    "gatsby-plugin-sharp": "^1.6.21",
    "gatsby-plugin-sitemap": "^1.2.5",
    "gatsby-plugin-twitter": "^1.0.12",
    "gatsby-remark-autolink-headers": "^1.4.8",
    "gatsby-remark-copy-linked-files": "^1.5.21",
    "gatsby-remark-images": "^1.5.32",
    "gatsby-remark-prismjs": "^1.2.9",
    "gatsby-remark-responsive-iframe": "^1.4.14",
    "gatsby-source-filesystem": "^1.5.8",
    "gatsby-transformer-remark": "^1.7.21",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-disqus-comments": "^1.1.1",
    "react-dom": "^16.1.1",
    "react-helmet": "^5.2.0",
    "react-share": "^1.17.0",
    "react-twitter-widgets": "^1.7.1",
    "remark-parse": "^4.0.0",
    "unified": "^6.1.6"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.1.0",
    "prettier": "^1.8.2",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "write-good": "^0.11.3"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
